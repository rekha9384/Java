import java.util.Scanner;
class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int sum = 0;
        for(int i=0; i<=n; i++){
            sum += i*3;
        }
        double avg = (double) sum / n;
        System.out.println("sum = " + sum);
        System.out.println("Avg = " + avg);
    }
}
